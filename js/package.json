{
  "name": "bqplot",
  "version": "0.5.23",
  "description": "bqplot",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab notebook",
    "jupyterlab-extension"
  ],
  "author": {
    "name": "BQPlot Development team",
    "email": "bqplot@bloomberg.net"
  },
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bloomberg/bqplot.git"
  },
  "scripts": {
    "build": "jlpm run build:css && jlpm run build:js && jlpm run build:labextension && webpack --mode=production",
    "build:css": "lessc less/bqplot.less css/bqplot.css",
    "build:js": "tsc",
    "build:labextension": "jupyter labextension build .",
    "watch": "npm-run-all -p watch:*",
    "watch:js": "tsc --watch",
    "watch:nbextension": "webpack --watch --mode=development",
    "watch:labextension": "jupyter labextension watch .",
    "prepare": "jlpm run build",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.3",
    "@babel/core": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@jupyter-widgets/controls": "^1.5.0 || ^2 || ^3",
    "@jupyterlab/builder": "^3.0.0",
    "@types/chai": "^4.1.7",
    "@types/d3": "^5.7.2",
    "@types/jest": "^26.0.20",
    "@types/node": "^13.13.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "css-loader": "^5.0.0",
    "file-loader": "^6.0.0",
    "imports-loader": "^0.8.0",
    "jest": "^26.6.3",
    "jest-raw-loader": "^1.0.1",
    "json-loader": "^0.5.7",
    "less": "^3.8.1",
    "less-loader": "^5.0.0",
    "npm-run-all": "^4.1.3",
    "raw-loader": "~4.0.1",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.0",
    "ts-jest": "^26.5.1",
    "typescript": "^3.5.2",
    "webpack": "^5",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^2 || ^3 || ^4.0.0",
    "@lumino/messaging": "^1",
    "@lumino/widgets": "^1",
    "d3": "^5.7.0",
    "d3-selection": "^1",
    "is-typedarray": "^1.0.0",
    "popper.js": "^1.0.0",
    "three": "^0.91.0",
    "topojson": "^1.6.24",
    "underscore": "^1.8.3"
  },
  "jupyterlab": {
    "extension": "lib/jupyterlab-plugin",
    "webpackConfig": "lab.webpack.config.js",
    "outputDir": "../share/jupyter/labextensions/bqplot",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      }
    }
  },
  "files": [
    "dist/",
    "css/",
    "lib/",
    "shaders/"
  ]
}
